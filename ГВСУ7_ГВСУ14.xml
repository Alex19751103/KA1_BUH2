				<ПослеЗагрузки>
МассивТипов = Новый Массив;
Для Каждого НаименованиеТипа Из ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ДокументыРасчетовСКонтрагентами.ТипЗначения.Типы() Цикл
	МассивТипов.Добавить(НаименованиеТипа);	
КонецЦикла;
ТипыДокументов = Новый ОписаниеТипов(МассивТипов);

СтруктураОтбора = Параметры.СтруктураОтбора;
СтруктураОтбора.Вставить("Организация",Параметры.Организация);
СтруктураОтбора.Вставить("НачПериода",Дата(1,1,1));
СтруктураОтбора.Вставить("КонПериода",Параметры.ДатаВвода);
СтруктураОтбора.Вставить("ТипыДокументов",ТипыДокументов);

Для каждого СтрокаТЧ из Объект.Движения.Хозрасчетный Цикл
	
	СоотвествиеДляЗаполненияСубконто = Новый Соответствие;
	Для каждого стр Из СтрокаТЧ.СубконтоДт Цикл
		СоотвествиеДляЗаполненияСубконто.Вставить(стр.Ключ,стр.Значение);
	КонецЦикла; 
	
	Для Сч = 1 по СтрокаТЧ.СчетДт.ВидыСубконто.Количество() Цикл
		Для каждого Элемент Из СоотвествиеДляЗаполненияСубконто Цикл
			ВидСубконто = СтрокаТЧ.СчетДт.ВидыСубконто[Сч - 1].ВидСубконто;
			Если ВидСубконто.ТипЗначения.СодержитТип(ТипЗнч(Элемент.Значение)) И ЗначениеЗаполнено(Элемент.Значение) Тогда
				СтрокаТЧ.СубконтоДт.Вставить(ВидСубконто,Элемент.Значение);
				Прервать;
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
	
	СоотвествиеДляЗаполненияСубконто = Новый Соответствие;
	Для каждого стр Из СтрокаТЧ.СубконтоКт Цикл
		СоотвествиеДляЗаполненияСубконто.Вставить(стр.Ключ,стр.Значение);
	КонецЦикла; 
	
	Для Сч = 1 по СтрокаТЧ.СчетКт.ВидыСубконто.Количество() Цикл
		Для каждого Элемент Из СоотвествиеДляЗаполненияСубконто Цикл
			ВидСубконто = СтрокаТЧ.СчетКт.ВидыСубконто[Сч - 1].ВидСубконто;
			Если ВидСубконто.ТипЗначения.СодержитТип(ТипЗнч(Элемент.Значение)) И ЗначениеЗаполнено(Элемент.Значение) Тогда
				СтрокаТЧ.СубконтоКт.Вставить(ВидСубконто,Элемент.Значение);
				Прервать;
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
	
	Если НЕ СтрокаТЧ.СчетДт.УчетПоПодразделениям Тогда
		СтрокаТЧ.ПодразделениеДт = Неопределено;
	КонецЕсли;
	
	Если НЕ СтрокаТЧ.СчетКт.УчетПоПодразделениям Тогда
		СтрокаТЧ.ПодразделениеКт = Неопределено;
	КонецЕсли;
	
	Если СтрокаТЧ.СчетДт.НалоговыйУчет Тогда
	Иначе	
		СтрокаТЧ.СуммаНУДт = 0;
		СтрокаТЧ.СуммаВРДт = 0;
		СтрокаТЧ.СуммаПРДт = 0;
	КонецЕсли;
	
	Если СтрокаТЧ.СчетКт.НалоговыйУчет Тогда
	Иначе
		СтрокаТЧ.СуммаНУКт = 0;
		СтрокаТЧ.СуммаВРКт = 0;
		СтрокаТЧ.СуммаПРКт = 0;
	КонецЕсли;
	
	Если НЕ СтрокаТЧ.СчетДт.Количественный Тогда
		СтрокаТЧ.КоличествоДт = 0;
	КонецЕсли;
	
	Если НЕ СтрокаТЧ.СчетКт.Количественный Тогда
		СтрокаТЧ.КоличествоКт = 0;
	КонецЕсли;
	
	Если НЕ СтрокаТЧ.СчетДт.Валютный Тогда
		СтрокаТЧ.ВалютаДт = "";
		СтрокаТЧ.ВалютнаяСуммаДт = 0;
	КонецЕсли;
	
	Если НЕ СтрокаТЧ.СчетКт.Валютный Тогда
		СтрокаТЧ.ВалютаКт = "";
		СтрокаТЧ.ВалютнаяСуммаКт = 0;
	КонецЕсли;	
	
	СтруктураОтбора.Вставить("Контрагент",СтрокаТЧ.СубконтоДт[ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Контрагенты]);
	СтруктураОтбора.Вставить("ДоговорКонтрагента",СтрокаТЧ.СубконтоДт[ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Договоры]);
	СтруктураОтбора.Вставить("СуммаДокумента",СтрокаТЧ.Сумма);
	
	Если СтрокаТЧ.СчетДт &lt;&gt; ПланыСчетов.Хозрасчетный.Вспомогательный И ЗначениеЗаполнено(СтруктураОтбора.Контрагент) И ЗначениеЗаполнено(СтруктураОтбора.ДоговорКонтрагента) Тогда
		Выполнить(Алгоритмы.ПодобратьДокументРасчетовСКонтрагентом);
		СтрокаТЧ.СубконтоДт.Вставить(ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ДокументыРасчетовСКонтрагентами,СтруктураОтбора.Документ);
